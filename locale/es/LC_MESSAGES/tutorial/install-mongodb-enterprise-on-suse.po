#: ../source/tutorial/install-mongodb-enterprise-on-suse.txt:22
msgid "``net-snmp``"
msgstr ""

# 4121acc30e8b45349ccaa0160d711156
#: ../source/tutorial/install-mongodb-enterprise-on-suse.txt:23
msgid "``snmp-mibs``"
msgstr ""

