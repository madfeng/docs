#: ../source/reference/command/serverStatus.txt:1192
# ec3c943f8a8c4bc380aa826a73ff3c67
msgid ":data:`~serverStatus.recordStats.pageFaultExceptionsThrown` reflects the number of page fault exceptions thrown by :program:`mongod` when accessing data for *all* databases managed by this :program:`mongod` instance."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1199
# 82a3806a73a14be38ba5befe75269038
msgid ":data:`~serverStatus.recordStats.local.accessesNotInMemory` reflects the number of times :program:`mongod` needed to access a memory page that was *not* resident in memory for the ``local`` database."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1205
# 06dac74fee934afab106cabd7f186100
msgid ":data:`~serverStatus.recordStats.local.pageFaultExceptionsThrown` reflects the number of page fault exceptions thrown by :program:`mongod` when accessing data for the ``local`` database."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1211
# 69247ff435b045d1902f48b070651d94
msgid ":data:`~serverStatus.recordStats.admin.accessesNotInMemory` reflects the number of times :program:`mongod` needed to access a memory page that was *not* resident in memory for the :term:`admin database`."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1217
# 9506488812c64226bade8f107d96f4e5
msgid ":data:`~serverStatus.recordStats.admin.pageFaultExceptionsThrown` reflects the number of page fault exceptions thrown by :program:`mongod` when accessing data for the :term:`admin database`."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1223
# 7821fb181eba4544aec4917da6dfedb1
msgid ":data:`~serverStatus.recordStats.<database>.accessesNotInMemory` reflects the number of times :program:`mongod` needed to access a memory page that was *not* resident in memory for the ``<database>`` database."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1229
# 3a3ec6da4cd04ef6943431df4ae32fa7
msgid ":data:`~serverStatus.recordStats.<database>.pageFaultExceptionsThrown` reflects the number of page fault exceptions thrown by :program:`mongod` when accessing data for the ``<database>`` database."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1237
# 64eed2cf334442e0bc963aeb69c92b92
msgid "workingSet"
msgstr ""

#: ../source/reference/command/serverStatus.txt:1241
# 4a67751d042b4c96ac6ebd0e307268a0
msgid "The :data:`~serverStatus.workingSet` data is only included in the output of :dbcommand:`serverStatus` if explicitly enabled. To return the :data:`~serverStatus.workingSet`, use one of the following commands:"
msgstr ""

#: ../source/reference/command/serverStatus.txt:1251
# 1010df1f9764491d92ea42af9665b8ec
msgid "For an example of the ``workingSet`` output, see the :ref:`workingSet section <server-status-example-workingset>` of the :doc:`/reference/server-status` page."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1257
# 272d8cb46c034bc28efe0e8c5ee79658
msgid ":data:`~serverStatus.workingSet` is a document that contains values useful for estimating the size of the working set, which is the amount of data that MongoDB uses actively. :data:`~serverStatus.workingSet` uses an internal data structure that tracks pages accessed by :program:`mongod`."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1265
# 165183cce17b4c608034a767f5a6415d
msgid ":data:`~serverStatus.workingSet.note` is a field that holds a string warning that the :data:`~serverStatus.workingSet` document is an estimate."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1271
# 7dc9f2957303478faa513500d7b6c4e8
msgid ":data:`~serverStatus.workingSet.pagesInMemory` contains a count of the total number of pages accessed by :program:`mongod` over the period displayed in :data:`~serverStatus.workingSet.overSeconds`. The default page size is 4 kilobytes: to convert this value to the amount of data in memory multiply this value by 4 kilobytes."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1278
# 6bfc247b24094423aa04d69e41f8fe03
msgid "If your total working set is less than the size of physical memory, over time the value of :data:`~serverStatus.workingSet.pagesInMemory` will reflect your data size."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1283
# 867038ec639145699b98c13c7a3f805b
msgid "Use :data:`~serverStatus.workingSet.pagesInMemory` in conjunction with :data:`~serverStatus.workingSet.overSeconds` to help estimate the actual size of the working set."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1289
# ead49b28f0c2467caa3c8bdb884a1fb1
msgid ":data:`~serverStatus.workingSet.computationTimeMicros` reports the amount of time the :program:`mongod` instance used to compute the other fields in the :data:`~serverStatus.workingSet` section."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1293
# 6ac576d0ca8042eebf14ab111a2bb1fb
msgid "Reporting on :data:`~serverStatus.workingSet` may impact the performance of other operations on the :program:`mongod` instance because MongoDB must collect some data within the context of a lock. Ensure that automated monitoring tools consider this metric when determining the frequency of collection for :data:`~serverStatus.workingSet`."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1302
# 411b4a556b354566964516ea8f11c6d0
msgid ":data:`~serverStatus.workingSet.overSeconds` returns the amount of time elapsed between the newest and oldest pages tracked in the :data:`~serverStatus.workingSet.pagesInMemory` data point."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1306
# fae076d5915d4877b870042d51076014
msgid "If :data:`~serverStatus.workingSet.overSeconds` is decreasing, or if :data:`~serverStatus.workingSet.pagesInMemory` equals physical RAM *and* :data:`~serverStatus.workingSet.overSeconds` is very small, the working set may be much *larger* than physical RAM."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1311
# eeb70ba28af346fc99c549e3c044d95c
msgid "When :data:`~serverStatus.workingSet.overSeconds` is large, MongoDB's data set is equal to or *smaller* than physical RAM."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1317
# 96ca504624cb4cdd934527b20716e5f3
msgid "metrics"
msgstr ""

#: ../source/reference/command/serverStatus.txt:1319
# 79415c5b760142cf8153d5f3f3025481
msgid "For an example of the metrics output, see the :ref:`metrics section <server-status-example-metrics>` of the :doc:`/reference/server-status` page."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1327
# ecba9dc2605b4cd4afdc6c5315dcbc95
msgid "The :data:`~serverStatus.metrics` document holds a number of statistics that reflect the current use and state of a running :program:`mongod` instance."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1333
# c73a0e885fe34823976f5c7afd54e320
msgid "The :data:`~serverStatus.metrics.document` holds a document of that reflect document access and modification patterns and data use. Compare these values to the data in the :data:`~serverStatus.opcounters` document, which track total number of operations."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1341
# 75e59b527c0044a88ee16dc87c42e3c7
msgid ":data:`~serverStatus.metrics.document.deleted` reports the total number of documents deleted."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1346
# b52eeb2d64c84e1280a29e489ecf739b
msgid ":data:`~serverStatus.metrics.document.inserted` reports the total number of documents inserted."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1351
# 0e60f407be3746a38f8a5c06fecf2086
msgid ":data:`~serverStatus.metrics.document.returned` reports the total number of documents returned by queries."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1356
# ee4bedf32fb7407e82736d939fe224c6
msgid ":data:`~serverStatus.metrics.document.updated` reports the total number of documents updated."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1361
# dbbbfdfb16b74a57b7e0050087adf89e
msgid ":data:`~serverStatus.metrics.getLastError` is a document that reports on :dbcommand:`getLastError` use."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1366
# fbbd00a27881432388e585de605bcd75
msgid ":data:`~serverStatus.metrics.getLastError.wtime` is a sub-document that reports :dbcommand:`getLastError` operation counts with a ``w`` argument greater than ``1``."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1372
# 8bf4e696d8184ec983b0bafb692ae68e
msgid ":data:`~serverStatus.metrics.getLastError.wtime.num` reports the total number of :dbcommand:`getLastError` operations with a specified write concern (i.e. ``w``) that wait for one or more members of a replica set to acknowledge the write operation (i.e. a ``w`` value greater than ``1``.)"
msgstr ""

#: ../source/reference/command/serverStatus.txt:1380
# b3bba0a169c8496982b5fbb3786a8337
msgid ":data:`~serverStatus.metrics.getLastError.wtime.totalMillis` reports the total amount of time in milliseconds that the :program:`mongod` has spent performing :dbcommand:`getLastError` operations with write concern (i.e.  ``w``) that wait for one or more members of a replica set to acknowledge the write operation (i.e. a ``w`` value greater than ``1``.)"
msgstr ""

#: ../source/reference/command/serverStatus.txt:1389
# 22540f6bce0042d8873c4d8b1b86ada2
msgid ":data:`~serverStatus.metrics.getLastError.wtimeouts` reports the number of times that :term:`write concern` operations have timed out as a result of the ``wtimeout`` threshold to :dbcommand:`getLastError`."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1396
# 6c0a54243d444e3d9e0ea3dbaaabe588
msgid ":data:`~serverStatus.metrics.operation` is a sub-document that holds counters for several types of update and query operations that MongoDB handles using special operation types."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1402
# 18bc1b82d69e473398ab8baab04a796b
msgid ":data:`~serverStatus.metrics.operation.fastmod` reports the number of :doc:`update </core/write-operations>` operations that neither cause documents to grow nor require updates to the index. For example, this counter would record an update operation that use the :update:`$inc` operator to increment the value of a field that is not indexed."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1411
# 8d42630e7ccb4002aad324905d5b2726
msgid ":data:`~serverStatus.metrics.operation.idhack` reports the number of queries that contain the ``_id`` field. For these queries, MongoDB will use default index on the ``_id`` field and skip all query plan analysis."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1418
# 3b991cb7efe6483d82df52a060f141ac
msgid ":data:`~serverStatus.metrics.operation.scanAndOrder` reports the total number of queries that return sorted numbers that cannot perform the sort operation using an index."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1424
# 8bbb8ba24a104849b827db7c05168855
msgid ":data:`~serverStatus.metrics.queryExecutor` is a document that reports data from the query execution system."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1429
# 7eb3db83a9a24a2b951837113315390c
msgid ":data:`~serverStatus.metrics.queryExecutor.scanned` reports the total number of index items scanned during queries and query-plan evaluation. This counter is the same as :data:`~explain.nscanned` in the output of :method:`~cursor.explain()`."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1436
# db91bed645954496879b5e975c75af07
msgid ":data:`~serverStatus.metrics.record` is a document that reports data related to record allocation in the on-disk memory files."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1441
# a730ebcbfea04db4824e10335991d86f
msgid ":data:`~serverStatus.metrics.record.moves` reports the total number of times documents move within the on-disk representation of the MongoDB data set. Documents move as a result of operations that increase the size of the document beyond their allocated record size."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1452
# c1d6dbb7d7014e6e8a06b2b72942adb1
msgid ":data:`~serverStatus.metrics.repl` holds a sub-document that reports metrics related to the replication process. :data:`~serverStatus.metrics.repl` document appears on all :program:`mongod` instances, even those that aren't members of :term:`replica sets <replica set>`."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1460
# 5bd3de10736d47c089e101a64ba82deb
msgid ":data:`~serverStatus.metrics.repl.apply` holds a sub-document that reports on the application of operations from the replication :term:`oplog`."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1466
# 3b3f8afbd0f6448d885c69b7f24eda61
msgid ":data:`~serverStatus.metrics.repl.apply.batches` reports on the oplog application process on :term:`secondaries <secondary>` members of replica sets. See :ref:`replica-set-internals-multi-threaded-replication` for more information on the oplog application processes"
msgstr ""

#: ../source/reference/command/serverStatus.txt:1474
# 85fcee4cb0c0426085dbb1898bb0ff3a
msgid ":data:`~serverStatus.metrics.repl.apply.batches.num` reports the total number of batches applied across all databases."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1479
# f2899d11c332457eb3a53ad9fe7c3c7b
msgid ":data:`~serverStatus.metrics.repl.apply.batches.totalMillis` reports the total amount of time the :program:`mongod` has spent applying operations from the oplog."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1485
# 5f5ef2bd6b58457ea3b7ffe1ded125cb
msgid ":data:`~serverStatus.metrics.repl.apply.ops` reports the total number of :term:`oplog` operations applied."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1490
# 901a9f6364194f01830c00904ff9c883
msgid "MongoDB buffers oplog operations from the replication sync source buffer before applying oplog entries in a batch. :data:`~serverStatus.metrics.repl.buffer` provides a way to track the oplog buffer. See :ref:`replica-set-internals-multi-threaded-replication` for more information on the oplog application process."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1499
# 6c5438c02f7d486c9a2ec3cd5772187c
msgid ":data:`~serverStatus.metrics.repl.buffer.count` reports the current number of operations in the oplog buffer."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1504
# 63aae5a8d158480b832834c7d90a61b5
msgid ":data:`~serverStatus.metrics.repl.buffer.maxSizeBytes` reports the maximum size of the buffer. This value is a constant setting in the :program:`mongod`, and is not configurable."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1510
# d8310e07e72d4462b2de186f10d72d4b
msgid ":data:`~serverStatus.metrics.repl.buffer.sizeBytes` reports the current size of the contents of the oplog buffer."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1515
# 4e9a014e6be64eaf92aae60db7a50c29
msgid ":data:`~serverStatus.metrics.repl.network` reports network use by the replication process."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1520
# a45a016ffea44647ae477a4fcd3d189e
msgid ":data:`~serverStatus.metrics.repl.network.bytes` reports the total amount of data read from the replication sync source."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1525
# a8231238bf724c33bbfb5ba206b9e079
msgid ":data:`~serverStatus.metrics.repl.network.getmores` reports on the ``getmore`` operations, which are requests for additional results from the oplog :term:`cursor` as part of the oplog replication process."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1534
# faa01875964d4735859941b96bc02e60
msgid ":data:`~serverStatus.metrics.repl.network.getmores.num` reports the total number of ``getmore`` operations, which are operations that request an additional set of operations from the replication sync source."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1541
# 58a181bb3db143efb441acf03bc0e20e
msgid ":data:`~serverStatus.metrics.repl.network.getmores.totalMillis` reports the total amount of time required to collect data from ``getmore`` operations."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1547
# a6bb4535824e449e96c9fcf6d36c329a
msgid "This number can be quite large, as MongoDB will wait for more data even if the ``getmore`` operation does not initial return data."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1553
# e2aaffc6fe7c4d7f9c602953796b7aef
msgid ":data:`~serverStatus.metrics.repl.network.ops` reports the total number of operations read from the replication source."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1558
# 266c349a59f0452fa2cba1185fbf7667
msgid ":data:`~serverStatus.metrics.repl.network.readersCreated` reports the total number of oplog query processes created. MongoDB will create a new oplog query any time an error occurs in the connection, including a timeout, or a network operation. Furthermore, :data:`~serverStatus.metrics.repl.network.readersCreated` will increment every time MongoDB selects a new source fore replication."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1568
# a7080e667b4d4118b2742cbafd8a94cd
msgid ":data:`~serverStatus.metrics.repl.oplog` is a document that reports on the size and use of the :term:`oplog` by this :program:`mongod` instance."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1574
# 6f2381ed37934db4849dbd53bac55087
msgid ":data:`~serverStatus.metrics.repl.oplog.insert` is a document that reports insert operations into the :term:`oplog`."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1579
# 26220d226f6c44c4a1a3411999c87803
msgid ":data:`~serverStatus.metrics.repl.oplog.insert.num` reports the total number of items inserted into the :term:`oplog`."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1584
# 988a6e0cb632411097729384517c6804
msgid ":data:`~serverStatus.metrics.repl.oplog.insert.totalMillis` reports the total amount of time spent for the :program:`mongod` to insert data into the :term:`oplog`."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1590
# 86b38754178846eb934cadf9d4cd1e5a
msgid ":data:`~serverStatus.metrics.repl.oplog.insertBytes` the total size of documents inserted into the oplog."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1595
# ea0d292b33a644f592d7140d6410162b
msgid ":data:`~serverStatus.metrics.repl.preload` reports on the \"pre-fetch\" stage, where MongoDB loads documents and indexes into RAM to improve replication throughput."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1599
# 9854a22b65e2424b900a91bfdf74aa6d
msgid "See :ref:`replica-set-internals-multi-threaded-replication` for more information about the *pre-fetch* stage of the replication process."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1605
# 820c49f7744b42d1a47d6ce7b259cfb9
msgid ":data:`~serverStatus.metrics.repl.preload.docs` is a sub-document that reports on the documents loaded into memory during the *pre-fetch* stage."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1611
# 1513f75c83bb4eed95c1d6ee75918866
msgid ":data:`~serverStatus.metrics.repl.preload.docs.num` reports the total number of documents loaded during the *pre-fetch* stage of replication."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1617
# 899a9ef7a62c4a81891699f11b17aaf3
msgid ":data:`~serverStatus.metrics.repl.preload.docs.totalMillis` reports the total amount of time spent loading documents as part of the *pre-fetch* stage of replication."
msgstr ""

#: ../source/reference/command/serverStatus.txt:1623
