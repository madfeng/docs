msgid "The server does not use SSL."
msgstr ""

#: ../source/includes/option/option-mongod-sslMode.rst:20
# 0e1306f865e64347aab4d38b74dc1885
msgid "``allowSSL``"
msgstr ""

#: ../source/includes/option/option-mongod-sslMode.rst:22
# 334990528c214af9b354e8f02bb70f99
